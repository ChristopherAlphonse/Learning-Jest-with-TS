{
  "extends":"./tslint.json" ,
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "sourceMap": true,
    "paths": {
      "*": ["./src/*"]
    },
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
      "noImplicitAny": true,
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
